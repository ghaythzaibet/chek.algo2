ALGORITHM set
VAR
    set1,set2 : ARRAY_OF INTEGER[length];
    [,]: INTEGER
    some : INTEGER:=0 ;
    found : BOOLEAN :
BEGIN
  FOR i FROM 0 TO set1 length-1 STEP step1 DO
  found,FALSE
FOR j FROM 0 TO set2 STEP2 length-1 step1 DO

    IF (set1[i]<> set2[j]) THEN
    found=TRUE
    END_IF
END_FOR
IF (found:=false) THEN
some:=some+set1[:];
   
END_IF
  END_FOR
END